{
  "cmp": {
    "switchControl": {
      "handler": {
        "elevation": {
          "boxShadow": {
            "enabled": {
              "value": "{alias.elevation.boxShadow.handler.base.enabled.value}"
            },
            "hovered": {
              "value": "{alias.elevation.boxShadow.handler.base.hovered.value}"
            },
            "pressed": {
              "value": "{alias.elevation.boxShadow.handler.base.pressed.value}"
            }
          }
        },
        "size": {
          "square": {
            "sm": {
              "value": "1.6rem"
            },
            "md": {
              "value": "2rem"
            },
            "lg": {
              "value": "2.4rem"
            }
          }
        },
        "shape": {
          "borderRadius": {
            "value": "{alias.shape.borderRadius.full.value}"
          }
        },
        "mutation": {
          "transitionDuration": {
            "value": "{alias.mutation.transitionDuration.translation.hor.sm.value}"
          }
        },
        "color": {
          "background": {
            "value": "{alias.color.background.handler.value}"
          },
          "text": {
            "base": {
              "value": "{alias.fields.color.text.base.weaker.value}"
            },
            "error": {
              "value": "{alias.fields.color.text.error.weaker.value}"
            },
            "warning": {
              "value": "{alias.fields.color.text.warning.weaker.value}"
            },
            "success": {
              "value": "{alias.fields.color.text.success.weaker.value}"
            }
          }
        }
      }
    }
  }
}
