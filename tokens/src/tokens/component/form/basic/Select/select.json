{
  "cmp": {
    "select": {
      "size": {
        "maxHeight": {
          "isMultiple": {
            "sm": {
              "value": "{alias.size.height.surface.lg.value}"
            },
            "md": {
              "value": "{alias.size.height.surface.xl.value}"
            },
            "lg": {
              "value": "{alias.size.height.surface.xxl.value}"
            }
          }
        }
      },
      "space": {
        "padding": {
          "scroll": {
            "value": "{alias.space.cmp.xs.value}"
          },
          "sortable": {
            "value": "{alias.space.cmp.lg.value}"
          }
        }
      }
    }
  }
}
