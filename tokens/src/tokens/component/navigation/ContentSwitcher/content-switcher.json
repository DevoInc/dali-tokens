{
  "cmp": {
    "contentSwitcher": {
      "space": {
        "gap": {
          "value": "{alias.space.cmp.xxs.value}"
        },
        "padding": {
          "value": "calc({alias.space.cmp.xs.value} / 2)"
        }
      },
      "size": {
        "height": {
          "sm": {
            "value": "calc({cmp.button.size.height.xs.value} + {alias.space.cmp.xs.value})"
          },
          "md": {
            "value": "calc({cmp.button.size.height.sm.value} + {alias.space.cmp.xs.value})"
          },
          "lg": {
            "value": "calc({cmp.button.size.height.md.value} + {alias.space.cmp.xs.value})"
          }
        }
      },
      "shape": {
        "borderRadius": {
          "value": "{cmp.button.shape.borderRadius.medium.value}"
        }
      },
      "color": {
        "background": {
          "base": {
            "value": "{alias.color.background.track.base.value}"
          }
        }
      }
    }
  }
}
