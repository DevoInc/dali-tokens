      "size": {
        "height": {
          "sm": {
            "value": "{alias.size.height.track.xs.value}"
          },
          "md": {
            "value": "calc({alias.size.height.track.sm.value} - 0.2rem)"
          },
          "lg": {
            "value": "calc({alias.size.height.track.md.value} - 0.2rem)"
          }
        }
      },
      "shape": {
        "borderRadius": {
          "value": "{alias.shape.borderRadius.pill.value}"
        },
        "opacity": {
          "base": {
            "value": "1"
          },
          "hovered": {
            "value": "{alias.shape.opacity.disabled.value}"
          }
        }
      },
      "space": {
        "gap": {
          "hasSeparators": {
            "sm": {
              "value": "calc({cmp.partitions.size.height.sm.value} / 2)"
            },
            "md": {
              "value": "calc({cmp.partitions.size.height.md.value} / 2)"
            },
            "lg": {
              "value": "calc({cmp.partitions.size.height.lg.value} / 2)"
            }
