{
  "cmp": {
    "mainMenu": {
      "navItemLink": {
        "mutation": {
          "transitionDuration": {
            "value": "all {alias.mutation.transitionDuration.action.value} easeIn-out"
          }
        },
        "space": {
          "ver": {
            "value": "0"
          },
          "hor": {
            "collapsed": {
              "value": "calc(({cmp.mainMenu.size.width.collapsed.value} - {cmp.mainMenu.navItemLinkIcon.size.square.value}) / 2)"
            },
            "expanded": {
              "value": "{alias.space.cmp.md.value}"
            }
          }
        },
        "typo": {
          "lineHeight": {
            "value": "{alias.typo.lineHeight.heading.h2.value}"
          },
          "fontWeight": {
            "value": "{alias.typo.fontWeight.heading.value}"
          },
          "fontSize": {
            "value": "{alias.typo.fontSize.heading.h5.value}"
          }
        }
      }
    }
  }
}
