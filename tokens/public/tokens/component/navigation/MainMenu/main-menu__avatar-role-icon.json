{
  "cmp": {
    "mainMenu": {
      "avatarRoleIcon": {
        "mutation": {
          "transitionDuration": {
            "value": "all {alias.mutation.transitionDuration.action.value} ease"
          }
        },
        "shape": {
          "borderRadius": {
            "value": "{alias.shape.borderRadius.full.value}"
          }
        },
        "size": {
          "height": {
            "value": "{alias.size.square.icon.base.xl.value}"
          },
          "width": {
            "value": "{alias.size.square.icon.base.xl.value}"
          }
        },
        "typo": {
          "fontSize": {
            "value": "{alias.typo.fontSize.lead.md.value}"
          }
        }
      }
    }
  }
}
