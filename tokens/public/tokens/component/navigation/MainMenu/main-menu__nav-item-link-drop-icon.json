{
  "cmp": {
    "mainMenu": {
      "navItemLinkDropIcon": {
        "mutation": {
          "transition": {
            "collapsed": {
              "value": "transform {alias.mutation.transitionDuration.rotation.180.value}, color {alias.mutation.transitionDuration.action.value}, opacity {alias.mutation.transitionDuration.opacity.sm.value}"
            },
            "expanded": {
              "value": "opacity {alias.mutation.transitionDuration.opacity.xxl.value} ease {alias.mutation.transitionDuration.opacity.xxl.value}"
            }
          }
        },
        "size": {
          "square": {
            "value": "{alias.typo.fontSize.icon.sm.value}"
          }
        },
        "typo": {
          "fontSize": {
            "value": "{alias.typo.fontSize.icon.xs.value}"
          }
        }
      }
    }
  }
}
