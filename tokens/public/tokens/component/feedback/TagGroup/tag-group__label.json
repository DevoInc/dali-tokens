{
  "cmp": {
    "tagGroup": {
      "label": {
        "size": {
          "height": {
            "sm": {
              "value": "calc({cmp.tag.size.height.sm.value} + {cmp.tagGroup.item.space.margin.ver.sm.value} * 2)"
            },
            "md": {
              "value": "calc({cmp.tag.size.height.md.value} + {cmp.tagGroup.item.space.margin.ver.md.value} * 2)"
            },
            "lg": {
              "value": "calc({cmp.tag.size.height.lg.value} + {cmp.tagGroup.item.space.margin.ver.lg.value} * 2)"
            }
          }
        },
        "space": {
          "marginRight": {
            "value": "{alias.space.cmp.xs.value}"
          },
          "marginBottom": {
            "sm": {
              "value": "calc({alias.space.cmp.xs.value} - {cmp.tagGroup.item.space.margin.ver.sm.value} / 2)"
            },
            "md": {
              "value": "calc({alias.space.cmp.xs.value} - {cmp.tagGroup.item.space.margin.ver.md.value} / 2)"
            },
            "lg": {
              "value": "calc({alias.space.cmp.xs.value} - {cmp.tagGroup.item.space.margin.ver.lg.value} / 2)"
            }
          }
        }
      }
    }
  }
}
