{
  "name": "@devoinc/genesys-tokens",
  "version": "7.5.2",
  "description": "Design tokens generator",
  "keywords": [
    "genesys",
    "design",
    "tokens"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com/DevoInc/genesys-tokens.git"
  },
  "license": "MIT",
  "author": "Devo, Inc. Quvis Team (https://github.com/orgs/DevoInc/teams/quvis)",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./tokens/**": "./tokens/**"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsup src/index.ts --dts --sourcemap --format esm,cjs",
    "postbuild": "cp -R src/tokens dist/",
    "clean": "rm -rf dist .cache",
    "dist": "npm run clean && npm run build",
    "start": "npm run build",
    "stryker": "stryker run"
  },
  "peerDependencies": {
    "quicktype-core": "23.x.x",
    "style-dictionary": "4.x.x"
  },
  "engine": {
    "node": ">=18",
    "npm": ">=8"
  },
  "publishConfig": {
    "access": "public"
  }
}
