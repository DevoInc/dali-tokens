{
  "cmp": {
    "mainMenu": {
      "secondary": {
        "mutation": {
          "transition": {
            "value": "{alias.mutation.transitionDuration.translation.hor.md.value} left ease-out"
          }
        },
        "size": {
          "width": {
            "value": "{alias.size.width.sidebar.lg.value}"
          }
        },
        "elevation": {
          "zIndex": {
            "value": "{alias.elevation.zIndex.depth.popOut.value}"
          },
          "boxShadow": {
            "value": "1px 0px 8px rgba(0, 0, 0, .48)"
          }
        },
        "color": {
          "background": {
            "value": "{brand.mainMenu.color.background.value}"
          },
          "text": {
            "value": "{brand.mainMenu.color.text.base.value}"
          }
        }
      }
    }
  }
}
