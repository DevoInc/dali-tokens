{
  "cmp": {
    "mainMenu": {
      "navSubItemLinkSocial": {
        "mutation": {
          "transition": {
            "value": "{alias.mutation.transitionDuration.action.value} background-color ease"
          }
        },
        "size": {
          "square": {
            "value": "0"
          }
        },
        "typo": {
          "fontSize": {
            "value": "{alias.typo.fontSize.icon.xs.value}"
          },
          "lineHeight": {
            "value": "0"
          }
        },
        "color": {
          "background": {
            "twitter": {
              "value": "{alias.color.background.social.twitter.value}"
            },
            "linkedin": {
              "value": "{alias.color.background.social.linkedin.value}"
            },
            "youtube": {
              "value": "{alias.color.background.social.youtube.value}"
            },
            "googlePlus": {
              "value": "{alias.color.background.social.googlePlus.value}"
            }
          },
          "text": {
            "enabled": {
              "value": "{alias.color.text.blendInverse.weak.value}"
            },
            "hovered": {
              "value": "{alias.color.text.body.inverse.value}"
            },
            "twitter": {
              "value": "{alias.color.text.social.twitter.value}"
            },
            "linkedin": {
              "value": "{alias.color.text.social.linkedin.value}"
            },
            "youtube": {
              "value": "{alias.color.text.social.youtube.value}"
            },
            "googlePlus": {
              "value": "{alias.color.text.social.googlePlus.value}"
            }
          }
        }
      }
    }
  }
}
