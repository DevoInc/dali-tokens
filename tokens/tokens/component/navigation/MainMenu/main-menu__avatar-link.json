{
  "cmp": {
    "mainMenu": {
      "avatarLink": {
        "mutation": {
          "transitionDuration": {
            "value": "all {alias.mutation.transitionDuration.action.value} ease"
          }
        },
        "space": {
          "marginTop": {
            "value": "{alias.space.cmp.xs.value}"
          }
        },
        "size": {
          "height": {
            "value": "100%"
          }
        },
        "color": {
          "background": {
            "enabled": {
              "value": "rgba(0,0,0,0)"
            },
            "hovered": {
              "value": "{alias.color.background.action.backdrop.inverse.hovered.value}"
            }
          }
        }
      }
    }
  }
}
